{
  "name": "node-api-boilerplate",
  "version": "0.0.1",
  "description": "Get up and running fast with this node api starter project. Built with Express and Sequelize.",
  "main": "bin/www",
  "scripts": {
    "start": "node bin/www",
    "build": "ls",
    "start:dev": "DEBUG=node-api-boilerplate:* ./node_modules/.bin/nodemon bin/www",
    "test": "mocha src/test --recursive --timeout 10000",
    "lint-fix": "eslint . --fix",
    "lint": "eslint .",
    "sequelize:migrate": "node_modules/.bin/sequelize db:migrate",
    "sequelize:seed": "node_modules/.bin/sequelize db:seed:all",
    "sequelize:drop": "node_modules/.bin/sequelize db:drop",
    "sequelize:create": "node_modules/.bin/sequelize db:create"
  },
  "author": "Bruce Stockwell",
  "license": "MIT",
  "dependencies": {
    "@types/chai-http": "^3.0.4",
    "@types/compression": "0.0.35",
    "@types/cors": "^2.8.3",
    "@types/debug": "0.0.30",
    "@types/dotenv-safe": "^4.0.1",
    "@types/express": "^4.11.1",
    "@types/http-status": "^0.2.30",
    "@types/joi": "^13.0.5",
    "@types/method-override": "0.0.31",
    "@types/sequelize": "^4.27.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "dotenv-safe": "^5.0.1",
    "eslint": "^4.18.1",
    "express": "^4.16.2",
    "express-paginate": "^0.3.0",
    "express-validation": "^1.0.2",
    "http-status": "^1.0.1",
    "joi": "^13.1.2",
    "jsonwebtoken": "^8.2.0",
    "method-override": "^2.3.10",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "pkginfo": "^0.4.1",
    "sequelize": "^4.34.1"
  },
  "devDependencies": {
    "chai-http": "^3.0.0",
    "chia": "0.0.1",
    "cross-conf-env": "^1.1.2",
    "eslint": "^4.18.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^5.0.1",
    "nodemon": "^1.15.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.0",
    "prettier-eslint": "^8.8.1"
  }
}
